{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static",
      "config": {
        "mimeTypes": {
          ".css": "text/css"
        }
      }
    },
    {
      "src": "*.js",
      "use": "@vercel/static",
      "config": {
        "mimeTypes": {
          ".js": "application/javascript"
        }
      }
    },
    {
      "src": "assets/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
